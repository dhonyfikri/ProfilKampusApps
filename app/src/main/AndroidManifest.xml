<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.profilkampus">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/campus_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/campus_logo_rounded"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProfilKampus">
        <activity android:name=".AccountActivity"></activity>
        <activity android:name=".ShowDetailCampusActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>